# ===== LLM Configuration (Hosted Lapathon API) =====
LAPA_URL=http://localhost:4000
API_KEY=secret_api-key
MODEL_NAME=lapa
TEMPERATURE=0.7
MAX_TOKENS=2048

# ===== Embeddings Configuration (Hosted Qwen) =====
USE_HOSTED_EMBEDDINGS=true
EMBEDDING_MODEL_NAME=text-embedding-qwen
EMBEDDING_DIMENSION=4096


# Use hosted embeddings API (faster setup, requires API key)
# Set to false to use local sentence-transformers
USE_HOSTED_EMBEDDINGS=true



# ===== Agent Configuration =====
MAX_CONVERSATION_HISTORY=20

# ===== Debug Settings =====
DEBUG_MODE=false
LOG_LEVEL=INFO

# ===== LangSmith Tracing =====
LANGCHAIN_TRACING_V2=true
LANGCHAIN_API_KEY=hello
LANGCHAIN_PROJECT=world

# ===== Phenix Tracing =====
ENABLE_PHOENIX=true
PHOENIX_COLLECTOR_ENDPOINT=http://localhost:6006
PHOENIX_PROJECT_NAME=graphiti-lapa-agent
# ===== Qdrant Config =====
QDRANT_URL=http://qdrant:6333
QDRANT_API_KEY= 
QDRANT_COLLECTION=facts
QDRANT_DISTANCE=cosine